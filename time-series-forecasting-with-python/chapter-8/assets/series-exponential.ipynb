   "source": [
    "# create and plot an exponential time series\n",
    "from matplotlib import pyplot\n",
    "from math import exp\n",
    "series = [exp(i) for i in range(1,10)]\n",
    "pyplot.figure(1)\n",
    "# line plot\n",
    "pyplot.subplot(211)\n",
    "pyplot.plot(series)\n",
    "# histogram\n",
    "pyplot.subplot(212)\n",
    "pyplot.hist(series)\n",
    "pyplot.show()"
   ]
  },
