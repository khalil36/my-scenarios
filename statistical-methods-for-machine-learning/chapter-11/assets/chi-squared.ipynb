   "source": [
    "# chi-squared percent point function\n",
    "from scipy.stats import chi2\n",
    "# define probability\n",
    "p = 0.55\n",
    "df = 5\n",
    "# retrieve value <= probability\n",
    "value = chi2.ppf(p, df)\n",
    "print(value)\n",
    "# confirm with cdf\n",
    "p = chi2.cdf(value, df)\n",
    "print(p)"
   ]
  },
