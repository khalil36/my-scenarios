    "# covariance matrix\n",
    "from numpy import array\n",
    "from numpy import cov\n",
    "# define matrix of observations\n",
    "X = array([\n",
    "\t[2, 3, 4],\n",
    "\t[4, 6, 8],\n",
    "\t[6, 9, 12],\n",
    "\t[8, 12, 16],\n",
    "\t[10, 15, 20]])\n",
    "print(X)\n",
    "# calculate covariance matrix\n",
    "Sigma = cov(X.T)\n",
    "print(Sigma)"
   ]
