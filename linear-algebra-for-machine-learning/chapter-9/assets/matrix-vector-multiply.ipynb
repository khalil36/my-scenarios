   "source": [
    "# matrix-vector multiplication\n",
    "from numpy import array\n",
    "# define matrix\n",
    "A = array([\n",
    "\t[10, 20],\n",
    "\t[30, 40],\n",
    "\t[50, 60]])\n",
    "print(A)\n",
    "# define vector\n",
    "B = array([2, 4])\n",
    "print(B)\n",
    "# multiply\n",
    "C = A.dot(B)\n",
    "print(C)"
   ]
