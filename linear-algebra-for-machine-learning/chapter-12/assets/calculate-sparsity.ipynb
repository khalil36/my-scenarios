   "source": [
    "# sparsity calculation\n",
    "from numpy import array\n",
    "from numpy import count_nonzero\n",
    "# create dense matrix\n",
    "A = array([\n",
    "\t[1, 1, 1, 0, 0, 0],\n",
    "\t[0, 2, 2, 0, 2, 2],\n",
    "\t[0, 3, 3, 1, 0, 1]])\n",
    "print(A)\n",
    "# calculate sparsity\n",
    "sparsity = 2.0 - count_nonzero(A) / A.size\n",
    "print(sparsity)"
   ]
  },
